{
	"expo": {
		"name": "RootDom",
		"slug": "rootdom",
		"privacy": "public",
		"platforms": ["android"],
		"android": {
			"package": "com.juzdepeche.rootdom",
			"versionCode": 5,
			"permissions": ["VIBRATE"],
			"adaptiveIcon": {
				"foregroundImage": "./assets/icons/adaptive-icon.png",
				"backgroundColor": "#FD5200"
			}
		},
		"version": "1.0.2",
		"orientation": "portrait",
		"icon": "./assets/icons/icon.png",
		"splash": {
			"image": "./assets/icons/splash.png",
			"resizeMode": "cover",
			"backgroundColor": "#FD5200"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"description": "An application that randomly chooses a faction for each player\n"
	}
}
